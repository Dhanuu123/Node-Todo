<html>
    <head>
        <title>Todo List</title>
        <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-KyZXEAg3QhqLMpG8r+Knujsl5+5hb7ieGZStb4m9gPo=" crossorigin="anonymous"></script>

        <link href="/assets/style.css" rel="stylesheet" type="text/css">

    </head>

    <body>
        <h1>Dhanuu's Todo List</h1>
        <div id="todo-table">
            <form>
                <input type="text" name="item" placeholder="Add new item.." required/>
                <button type = "submit">Add Item</button>
            </form>
                <ul>
                   <% for(var i=0;i<todos.length;i++){ %>
                     <li><%= todos[i].item %></li>
                   <% } %>
                </ul>
        </div>
    </body>
</html>